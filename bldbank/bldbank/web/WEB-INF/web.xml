<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hregister</servlet-name>
        <servlet-class>MyPack.hregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bregister</servlet-name>
        <servlet-class>MyPack.bregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>MyPack.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchDoner</servlet-name>
        <servlet-class>MyPack.SearchDoner</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hregister</servlet-name>
        <url-pattern>/hregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bregister</servlet-name>
        <url-pattern>/bregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchDoner</servlet-name>
        <url-pattern>/SearchDoner</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
